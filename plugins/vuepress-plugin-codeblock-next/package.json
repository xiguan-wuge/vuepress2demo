{
  "name": "vuepress-plugin-codeblock-next",
  "version": "1.0.3",
  "description": "基于vuepress-plugin-demoblock-plus对长期支持版本的vuepress2.0x进行适配",
  "license": "MIT",
  "type": "module",
  "author": "sunbyte",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "vuepress2",
    "vuepress",
    "vue",
    "plugin"
  ],
  "main": "dist/node/index.mjs",
  "module": "dist/node/index.cjs",
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "require": "./dist/node/index.cjs",
      "import": "./dist/node/index.mjs"
    }
  },
  "scripts": {
    "docs:build": "vuepress build docs",
    "docs:clean-dev": "vuepress dev docs --clean-cache",
    "docs:dev": "vuepress dev docs",
    "docs:update-package": "npx vp-update",
    "build": "unbuild"
  },
  "dependencies": {
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@mdit-vue/plugin-sfc": "^0.11.1",
    "@vue/compiler-sfc": "3.2.44",
    "@vuepress/bundler-vite": "^2.0.0-rc.7",
    "@vuepress/client": "^2.0.0-rc.20",
    "@vuepress/plugin-shiki": "2.0.0-beta.51",
    "@vuepress/theme-default": "^2.0.0-rc.11",
    "@vuepress/utils": "^2.0.0-rc.20",
    "@vueuse/core": "^12.7.0",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "vue": "^3.4.0",
    "vue-router": "^4.5.0",
    "vuepress": "^2.0.0-rc.7"
  },
  "config": {
    "configExt": ".ts"
  },
  "devDependencies": {
    "@vue/babel-plugin-jsx": "^1.3.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^22.13.5",
    "@yunquejs/release": "^0.1.4",
    "chokidar-cli": "^3.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "element-plus": "^2.9.5",
    "less": "^4.1.2",
    "picocolors": "latest",
    "sass": "^1.85.1",
    "sass-embedded": "^1.85.1",
    "sass-loader": "^16.0.5",
    "tsx": "latest",
    "typescript": "^4.8.2",
    "unbuild": "0.9.4",
    "unicorn-magic": "^0.1.0"
  }
